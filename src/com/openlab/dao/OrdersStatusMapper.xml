<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.openlab.dao.OrdersStatusMapper">
	<update id="updateOrdersdetailStatus" parameterType="map">
		update ordersdetail set od_status = #{od_status} where od_id = #{od_id}
	</update>
	
	<update id="updateOrdersStatus" parameterType="map">
		update orders set o_status = #{o_status} where o_id = #{o_id}
	</update>
</mapper>
