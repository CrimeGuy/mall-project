<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.openlab.dao.GoodsStatusMapper">
	<update id="updateGoodsStatus" parameterType="map">
		update goods set g_status = #{g_status} where g_id = #{g_id}
	</update>
	<update id="updateCarStatus" parameterType="map">
		update car set c_status = #{g_status} where g_id = #{g_id}
	</update>
</mapper>
